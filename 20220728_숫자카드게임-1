1	n, m = map(int, input().split())
2	result = 0
3	
4	for i in range(n):
5	    data = list(map(int, input().split()))
6	    min_val = 10001
7	    for a in data:
8	        min_val = min(min_val, a)
9	        
10	    result = max(result, min_val)
11	    
12	print(result)

## output
3 3
3 1 2
4 1 4
2 2 2
2
